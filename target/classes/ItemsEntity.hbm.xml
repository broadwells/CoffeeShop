<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.grandcircus.spring.models.ItemsEntity" table="Items" schema="CoffeeShopDB">
        <id name="itemId">
            <column name="ItemID" sql-type="int(11)"/>
        </id>
        <property name="product">
            <column name="Product" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="description">
            <column name="Description" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="quantity">
            <column name="Quantity" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="price">
            <column name="Price" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>